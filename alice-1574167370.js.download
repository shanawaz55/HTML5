        $(".requiredrating").each(function() {
            $(this).on('blur',function(){
			var dataInput = $(this).data("rating");
			var dataInputVal = $(this).val().trim();
                        var dataSelectVal = $(this).find('option:selected').val();
			switch (dataInput) {
				case "rating-textarea":
