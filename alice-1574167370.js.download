            if(data){
                showHidePCLoader(false);
                pincodeToggle(true);
                var searchTerm = pincode.replace(/[(?=+*'? ]/g, '');
                var trimmedSearchInput = $.trim(searchTerm);
                var obj = $.parseJSON(data);
