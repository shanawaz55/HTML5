function isMobile(){
	var is_mobile = false;
	
	var w = window,
		d = document,
		e = d.documentElement,
		g = d.getElementsByTagName('body')[0],
		x = w.innerWidth || e.clientWidth || g.clientWidth,
		y = w.innerHeight|| e.clientHeight|| g.clientHeight;	
	if( x < 767) {
		is_mobile = true;
	}

    return is_mobile;	
}

var detectIsMobile = function() {
    var is_mobile = false;
    var w = window,
        d = document,
        e = d.documentElement,
        g = d.getElementsByTagName('body')[0],
        x = w.innerWidth || e.clientWidth || g.clientWidth,
        y = w.innerHeight|| e.clientHeight|| g.clientHeight;
    if( x < 767) {
        is_mobile = true;
    }

    return is_mobile;
};

